import java.util.*;
class hey
{
    public static void main(String args[])
    {
    ArrayList <Integer> a=new ArrayList<Integer>();
    a.add(2);
    a.add(4);
    a.add(6);
     ArrayList<Integer> b=new ArrayList<Integer>();
     a.add(8);
     a.add(10);
     a.add(12);
       ArrayList<Integer> c=new ArrayList<Integer>();
       c.addAll(a);
       c.addAll(b);
       for(int i:c)
       {
           System.out.println(i);
       }
    }
}
