import java.util.*;
class copy
{
    public static void main(String args[])
    {
    ArrayList <Integer> a=new ArrayList<Integer>();
    a.add(2);
    a.add(4);
    a.add(6);
    System.out.println("arraylist before clear:"+a);
    a.clear();
     System.out.println("arraylist after clear:"+a);
    }
}
